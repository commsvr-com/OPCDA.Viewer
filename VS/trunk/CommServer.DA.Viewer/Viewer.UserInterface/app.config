<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="CAS.OPCViewer.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>

  <userSettings>
    <CAS.OPCViewer.Properties.Settings>
      <setting name="DebugPaneOn" serializeAs="String">
        <value>True</value>
      </setting>
    </CAS.OPCViewer.Properties.Settings>
  </userSettings>

  <system.diagnostics>
    <sources>
      <source name="TracesFromEventLogMonitor" switchName="EventLogMonitor" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
      <source name="CAS.OPCViewer" switchName="OPCClient.Da" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
      <source name="CAS.Lib.OPCClientControlsLib" switchName="OPCClient.Da" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
      <source name="OpcCom.Tracer" switchName="OPCClient.Da" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
      <source name="Opc.Tracer" switchName="OPCClient.Da" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
      <source name="CAS.Lib.OPCClient.Da" switchName="OPCClient.Da" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
      <source name="CAS.Lib.ControlLibrary.TraceEvent" switchName="CAS.Lib.ControlLibrary.TraceEvent" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
      <source name="CAS.BaseStation.Transactions" switchName="CAS_BaseStation_Transactions" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>

      <!--CAS-->
      <source name="CAS.CodeProtect.TraceSource" switchName="CAS.CodeProtect.TraceSource.Switch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
      <source name="CAS.RealTime" switchName="CAS.RealTime.Switch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
      <source name="CAS.Windows.Forms" switchName="CAS.Windows.Forms.Switch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
      <source name="CAS.CommServer.CommonBus" switchName="CAS.CommServer.CommonBus.Switch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
      <!--CAS.CommServer.DA.Viewer-->
      <source name="CAS.CommServer.DA.Viewer.FormControls" switchName="CAS.CommServer.DA.Viewer.FormControls.Switch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
      <source name="CAS.CommServer.DA.Viewer.UserInterface" switchName="CAS.CommServer.DA.Viewer.UserInterface.Switch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
    </sources>
    
    <switches>
      <!-- SourceLevels Enumeration -->
      <add name="EventLogMonitor" value="Warning" />
      <add name="CAS_BaseStation_Transactions" value="Warning" />
      <add name="CAS.Lib.ControlLibrary.TraceEvent" value="Information" />

      <!--CAS-->
      <add name="CAS.CodeProtect.TraceSource.Switch" value="All" />
      <add name="CAS.RealTime.Switch" value="All" />
      <add name="CAS.Windows.Forms.Switch" value="All" />
      <add name="CAS.CommServer.CommonBus.Switch" value="All" />
      <!--CAS.CommServer.DA.Viewer-->
      <add name="CAS.CommServer.DA.Viewer.FormControls.Switch" value="All" />
      <add name="CAS.CommServer.DA.Viewer.UserInterface.Switch" value="All" />
    </switches>
    <sharedListeners>
      <add name="LogFile" type="CAS.Lib.CodeProtect.EnvironmentAccess.AdvancedDelimitedListTraceListener,CAS.CodeProtect" initializeData="|ApplicationDataPath|\CAS.CommServer.CommonBus.UnitTests.log" traceOutputOptions="DateTime">
        <filter type="System.Diagnostics.EventTypeFilter" initializeData="All" />
      </add>
      <add name="OPCViewer.Output.TraceListener" type="CAS.Lib.ControlLibrary.TextBoxBaseTraceListener,CAS.Windows.Forms">
        <filter type="System.Diagnostics.EventTypeFilter" initializeData="All" />
      </add>
      <!--<add name="ConfigLogFile" type="System.Diagnostics.DelimitedListTraceListener" initializeData ="log\NetworkConfig.log" traceOutputOptions="DateTime">
        <filter type="System.Diagnostics.EventTypeFilter" initializeData="All" />
      </add>-->
    </sharedListeners>
  </system.diagnostics>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>
</configuration>
